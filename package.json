{
  "name": "stock-price-checker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "predev": "dotenv-checker --schema .env.example --env .env",
    "dev": "docker-compose up -d && nodemon src/index.ts",
    "start": "docker-compose up -d && node dist/index.js",
    "build": "npx tsc",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src",
    "lint": "eslint --fix src",
    "prepare": "husky install",
    "migration:generate": "drizzle-kit generate:pg",
    "migration:run": "tsx ./src/db/scripts/migrate.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "lint-staged": {
    "./src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.4",
    "express": "^4.19.1",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "tsx": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.11.4",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "dotenv-checker": "^1.1.5",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
